  <image src='img/menu.png' class='menu'></image>
    <view class='mi-tab-menu'>
      <view class='mi-tab-menu-item active'>
        <text>盛夏\n如花</text>
      </view>
      <view class='mi-tab-menu-item'>
        <text>高山\n流水</text>
      </view>
      <view class='mi-tab-menu-item'>
        <text>阵阵\n酥麻</text>
      </view>
      <view class='mi-tab-menu-item'>
        <text>小鹿\n乱撞</text>
      </view>
      <view class='mi-tab-menu-item'>
        <text>浪花\n迭起</text>
