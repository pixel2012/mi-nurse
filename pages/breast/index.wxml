                </view>
              </view>
            </view>
          </view>
        </swiper-item>
        <!--第三步  -->
        <swiper-item>
          <view class='mi-swiper-item'>
            <view class='mi-step'>
              <image src='img/dot.png' style='width:10rpx;height:10rpx;margin:0 25rpx;'></image>
              当前正处于第
              <text class='mi-main-color'>1</text> 个循环的第
