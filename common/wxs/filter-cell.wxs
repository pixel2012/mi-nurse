                  return 'cell-active-60.png';
                }
                else
                  if (num > 60 && num <= 70) {
                    return 'cell-active-70.png';
                  }
                  else
                    if (num > 70 && num <= 80) {
                      return 'cell-active-80.png';
                    }
                    else
                      if (num > 80 && num <= 90) {
