  if (!con) {
    console.log('未连接');
    return 'cell.png';
  } else {
    console.log('已连接');
    
    if (num == 0) {
      return 'cell-active-0.png';
    }
    else
      if (num > 0 && num <= 10) {
        return 'cell-active-10.png';
      }
      else
        if (num > 10 && num <= 20) {
          return 'cell-active-20.png';
        }
        else
          if (num > 20 && num <= 30) {
            return 'cell-active-30.png';
          }
          else
            if (num > 30 && num <= 40) {
              return 'cell-active-40.png';
            }
            else
              if (num > 40 && num <= 50) {
                return 'cell-active-50.png';
              }
              else
